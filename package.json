{
  "name": "noutore-biyori",
  "version": "1.0.0",
  "description": "脳トレ・クイズに特化したWebメディア「脳トレ日和」のフロントエンド（SvelteKit）リポジトリです。",
  "type": "module",
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "dev": "svelte-kit dev",
    "prebuild": "pnpm exec svelte-kit --version && pnpm exec svelte-kit --help",
    "build": "pnpm exec svelte-kit build",
    "preview": "pnpm exec svelte-kit preview",
    "test": "echo \"no tests yet\" && exit 0",
    "format": "pnpm dlx prettier --write .",
    "format:check": "pnpm dlx prettier --check ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NBWMedia0807/noutore-biyori.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NBWMedia0807/noutore-biyori/issues"
  },
  "homepage": "https://github.com/NBWMedia0807/noutore-biyori#readme",
  "dependencies": {
    "@sanity/client": "7.9.0",
    "@sanity/image-url": "1.1.0",
    "@sveltejs/kit": "2.36.2",
    "vite": "7.1.3"
  },
  "devDependencies": {
    "svelte": "5.38.3",
    "@sveltejs/adapter-vercel": "5.10.2"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["esbuild"]
  }
}
